@model Committee.Models.CommitteeViewModel

@if (!string.IsNullOrEmpty(Model.ErrorMessage))
{
<div class="alert alert-danger" role="alert">
    @Model.ErrorMessage
</div>
}
else
{
<h2>@Model.DisplayNumber</h2>
<br />
<br />
    if (Model.CommitteeMembers.Count > 1)
    {
<b>Other Members</b>
<div class=" table-responsive">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <td>
                    <b>Name</b>
                </td>
                <td>
                    <b>Number</b>
                </td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.CommitteeMembers)
                    {
            <tr>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Number
                </td>
            </tr>
                    }
        </tbody>
    </table>
</div>
    }
}
